# Example environment configuration for ethicalmeat-db
# Copy this file to .env and customize as needed

# Cache configuration
CACHE_DIR=cache
ENABLE_CACHE=true

# Rate limiting (seconds between requests)
RATE_LIMIT=1.0

# Output configuration
OUTPUT_FORMAT=both  # Options: json, csv, both
OUTPUT_FILENAME=emh_ratings

# Scraper settings
BASE_URL=https://essenmitherz.ch
USER_AGENT=Mozilla/5.0 (compatible; EthicalMeatScraper/0.1.0)

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
